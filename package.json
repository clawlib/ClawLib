{
    "name": "clawlib",
    "version": "1.0.0",
    "description": "Unified AI Agent Framework for the Claw ecosystem",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "dev": "tsup src/index.ts --format cjs,esm --watch --dts",
        "test": "vitest",
        "example": "tsx examples/basic-agent.ts"
    },
    "keywords": [
        "ai",
        "agent",
        "claw",
        "automation",
        "sdk",
        "llm",
        "orchestrator"
    ],
    "author": "Antigravity",
    "license": "MIT",
    "dependencies": {
        "@anthropic-ai/sdk": "^0.26.1",
        "@google/generative-ai": "^0.14.0",
        "@mistralai/mistralai": "^0.1.3",
        "chalk": "^4.1.2",
        "dotenv": "^16.4.5",
        "events": "^3.3.0",
        "groq-sdk": "^0.5.0",
        "node-os-utils": "^1.3.7",
        "nodemailer": "^6.9.9",
        "openai": "^4.52.7",
        "ora": "^5.4.1",
        "playwright": "^1.45.1",
        "qrcode-terminal": "^0.12.0",
        "readline-sync": "^1.4.10",
        "telegraf": "^4.16.3",
        "whatsapp-web.js": "^1.23.0",
        "zod": "^3.23.8",
        "zod-to-json-schema": "^3.23.2"
    },
    "devDependencies": {
        "@types/node": "^20.11.20",
        "@types/nodemailer": "^7.0.11",
        "@types/readline-sync": "^1.4.8",
        "tsup": "^8.0.2",
        "tsx": "^4.7.1",
        "typescript": "^5.3.3",
        "vitest": "^1.3.1"
    }
}
